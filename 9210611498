local values = game:GetService("Workspace").Players.noreportmepls89.Handler.Values.Thirst
local Thirst = values.Thirst.Value
local stamina = values.MaxStamina.CurrentStamina.Value
local hunger = values.Hunger.Value

while wait() do
    stamina = stamina + 1
    hunger = hunger + 1
    Thirst = Thirst + 1
end
